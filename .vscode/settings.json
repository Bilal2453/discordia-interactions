{
  "Lua.diagnostics.disable": [
    "undefined-doc-class",
    "undefined-doc-name"
  ],
  "Lua.runtime.path": [
    "?.lua",
    "?/init.lua",
    "deps/?/init.lua",
    "deps/?.lua",
    "libs/?.lua"
  ],
  "Lua.workspace.library": [
    "../deps",
    "../libs",
    "${addons}/luvit/module/library"
  ],
  "Lua.diagnostics.globals": [
    "p",
    "args"
  ],
  "Lua.workspace.checkThirdParty": false
}